type Query {
    getSoldProducts: [Product]
    getStoreByIdAndName(id: ID!, name: String!): Store
}

type Store @key(fields: "id name") {
    id: ID
    name: String
    sold: [Product]
    stock: [Product]
    personOfContact: Person
}

type Product {
    name: String
}

type Person {
    id: ID
    firstName: String
    lastName: String
}